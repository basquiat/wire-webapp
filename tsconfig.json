{
  "compilerOptions": {
    "declaration": false,
    "downlevelIteration": true,
    "jsx": "react",
    "lib": ["dom", "es2017"],
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "outDir": "app/script/auth",
    "rootDir": "app/script",
    "sourceMap": true,
    "target": "ES6"
  },
  "exclude": ["node_modules"]
}
